{
  "schemaVersion": 1,
  "id": "jei",
  "version": "${version}",
  "name": "Just Enough Items",
  "description": "JEI is an item and recipe viewing mod for Minecraft, built from the ground up for stability and performance. (kobosh1)",
  "authors": [
    "Alexander01998",
    "kobosh"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/jei",
    "sources": "https://github.com/mezz/JustEnoughItems"
  },
  "license": "GPL-3.0-or-later",
  "icon": "assets/jei/jei-icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "net.wurstclient.WurstInitializer",
      "net.wurstclient.test.WurstE2ETestClient"
    ]
  },
  "mixins": [
    "wurst.mixins.json"
  ],
  "accessWidener": "wurst.accesswidener",
  "depends": {
    "fabricloader": ">=0.15.10",
    "fabric-api": ">=0.107.0",
    "minecraft": "~1.21-beta.1",
    "java": ">=21"
  },
  "suggests": {
    "mo_glass": "*"
  },
  "breaks": {
    "wi_zoom": "*",
    "wi-zoom": "*",
    "vulkanmod": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.donate": "https://www.wurstclient.net/modmenu-donate/",
        "modmenu.twitter": "https://www.wurstclient.net/modmenu-twitter/",
        "modmenu.wiki": "https://www.wurstclient.net/modmenu-wiki/",
        "Forum": "https://www.wurstclient.net/modmenu-forum/"
      }
    }
  }
}
